{
	"logs": {
		"credentials": {
			"role_arn": "arn:aws:iam::146797033453:role/aws-cloudwatch-RoleToReceiveLogs"
		},
        "logs_collected": {
          	"files": {
	            "collect_list": [
	            	{
	                "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
	                "log_group_name": "aws-cloudwatchagent-logs/system/",
	                "log_stream_name": "{hostname}_cwagent.log",
	                "timezone": "Local"
	              	},
	              	{
	                "file_path": "/var/log/messages",
	                "log_group_name":  "aws-cloudwatchagent-logs/system/",
	                "log_stream_name": "{hostname}_var-log-messages",
	                "timezone": "Local"
	              	},
	              	{
	                "file_path": "/var/log/secure",
	                "log_group_name":  "aws-cloudwatchagent-logs/system/",
	                "log_stream_name": "{hostname}_var-log-secure",
	                "timezone": "Local"
	              	},
	              	{
	                "file_path": "/var/log/yum.log",
	                "log_group_name":  "aws-cloudwatchagent-logs/system/",
	                "log_stream_name": "{hostname}_var-log-yum",
	                "timezone": "Local"
	              	},
	              	{
	                "file_path": "/var/log/audit/audit.log",
	                "log_group_name":  "aws-cloudwatchagent-logs/system/",
	                "log_stream_name": "{hostname}_var-log-audit",
	                "timezone": "Local"
	              	}
	            ]
	          }
	        },
			"log_stream_name": "/ec2/catchall"
    }
}